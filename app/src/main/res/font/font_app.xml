<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <font
        android:font="@font/sf_pro_text_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/sf_pro_text_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <font
        android:font="@font/sf_pro_text_medium"
        android:fontStyle="italic"
        android:fontWeight="400"
        app:font="@font/sf_pro_text_medium"
        app:fontStyle="italic"
        app:fontWeight="400" />

    <!--bold-->
    <font
        android:font="@font/sf_pro_text_semibold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/sf_pro_text_semibold"
        app:fontStyle="normal"
        app:fontWeight="700" />
</font-family>